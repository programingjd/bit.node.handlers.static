{
  "name": "bit.node.handlers.static",
  "version": "1.0.3",
  "description": "nodejs http handler for serving static files",
  "main": "static.js",
  "scripts": {
    "preinstall": "npm list -g bit-bin -g || npm install bit-bin -g --unsafe-perm",
    "test": "mocha test/static.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programingjd/bit.node.handlers.static.git"
  },
  "engines": {
    "node": ">=11.7.0"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/programingjd/bit.node.handlers.static/issues"
  },
  "homepage": "https://github.com/programingjd/bit.node.handlers.static#readme",
  "bit": {
    "env": {
      "tester": "bit.envs/testers/mocha@5.0.2"
    },
    "componentsDefaultDirectory": "lib/{name}",
    "dependenciesDirectory": "test",
    "saveDependenciesAsComponents": true,
    "packageManager": "npm",
    "overrides": {
      "static.js": {
        "devDependencies": {
          "@bit.internal/data": "+"
        }
      }
    }
  },
  "devDependencies": {
    "mocha": "^6.2.1"
  }
}
